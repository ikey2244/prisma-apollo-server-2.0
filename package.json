  "dependencies": {
    "apollo-server": "^2.9.3",
    "dotenv": "^8.1.0",
    "env-cmd": "^10.0.1",
    "graphql": "^14.5.4",
    "graphql-import": "^0.7.1",
    "prisma-client-lib": "^1.34.8"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "nodemon": "^1.19.2"
  }
