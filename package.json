{
  "name": "prisma-apollo2",
  "version": "1.0.0",
  "description": "prisma apollo 2 server boilerplate",
  "main": "index.js",
  "scripts": {
    "deploy": "prisma deploy --env-file .env",
    "start": "env-cmd -f .env nodemon src/index.js --ext js,graphql --exec babel-node"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ikey2244/prisma-apollo2.git"
  },
